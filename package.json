{
	"name": "run-in-terminal",
	"displayName": "Run Script in Terminal",
	"description": "run script in terminal",
	"publisher": "tingyu",
	"repository": {
		"type": "git",
		"url": "https://github.com/TingyuShare/run-in-terminal"
	},
	"version": "0.0.8",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"terminal",
		"shell",
		"selection",
		"copy",
		"send",
		"env",
		"bash",
		"zsh"
	],
	"markdown": "github",
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
    "icon": "images/icon.png",
	"contributes": {
		"commands": [
			{
				"command": "runInTerminal.select-selection",
				"title": "RunInTerminal: Selected selection"
			},
			{
				"command": "runInTerminal.active-file",
				"title": "RunInTerminal: Active file"
			},
			{
				"command": "runInTerminal.whatisMyip",
				"title": "RunInTerminal: WhatIsMyIP"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.63.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}
